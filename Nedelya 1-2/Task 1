//1. Вывести на экран Первую букву имени из символов "*" (звездочка)
#include <iostream>

using namespace std;

int main()
{

    std::cout << "  ****** " << std::endl
        << "  *******  " << std::endl
        << "  ****** " << std::endl
        << "  ** " << std::endl
        << "  ** " << std::endl;
    std::getchar(); 
    return 0;
}
